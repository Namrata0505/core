package Com;

public class StringCount {
static void countString(String a){
	char[] ch= a.toCharArray();
	int count=1;
	for(int i=0;i<ch.length;i++) {
		
	for(int j=1;j<ch.length;j++) {
			if(ch[i]==ch[j])
			System.out.print(ch[i]+" "+count++);
		}
		
	
	}

	
	
	
}
	public static void main(String[] args) {
countString("Hello");





}


}
